    <section id="contact" class="contact">
        <div class="section-heading text-center">
            <h2>İLETİŞİM</h2>
        </div>
        <div class="container">
            <div class="contact-content">
                <div class="row">
                    <div class="col-md-offset-1 col-md-5 col-sm-6">
                        <div class="single-contact-box">
                            <div class="contact-form">
                                @using (Html.BeginForm("Index", "Index",FormMethod.Post)) 
                                { 
                                    <div class="row">
                                        <div class="col-sm-12 col-xs-12">
                                            <div class="form-group">
                                                <p style="color:green">@ViewBag.Mesaj</p>
                                            </div><!--/.form-group-->
                                        </div><!--/.col-->
                                    </div><!--/.row-->

                                    <div class="row">
                                        <div class="col-sm-6 col-xs-12">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="name" placeholder="Adınız" name="name" required maxlength="100">
                                            </div><!--/.form-group-->
                                        </div><!--/.col-->
                                        <div class="col-sm-6 col-xs-12">
                                            <div class="form-group">
                                                <input type="email" class="form-control" id="email" placeholder="Mail adresiniz" name="email" required maxlength="100">
                                            </div><!--/.form-group-->
                                        </div><!--/.col-->
                                    </div><!--/.row-->
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="subject" placeholder="Konu" name="subject" required maxlength="200">
                                            </div><!--/.form-group-->
                                        </div><!--/.col-->
                                    </div><!--/.row-->
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <textarea class="form-control" rows="8" id="comment" placeholder="Mesaj" name="message" required maxlength="1000"></textarea>
                                            </div><!--/.form-group-->
                                        </div><!--/.col-->
                                    </div><!--/.row-->
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="single-contact-btn">
                                                <button class="contact-btn" type="submit" role="button">Gönder</button>
                                            </div><!--/.single-single-contact-btn-->
                                        </div><!--/.col-->
                                    </div><!--/.row-->
                                }
                                <!--/form-->
                            </div><!--/.contact-form-->
                        </div><!--/.single-contact-box-->
                    </div><!--/.col-->
                    <div class="col-md-offset-1 col-md-5 col-sm-6">
                        <div class="single-contact-box">
                            <div class="contact-adress">
                                <div class="contact-add-head">
                                    <h3>CvUpp</h3>
                                    <p>Kariyer Rehberiniz</p>
                                </div>
                                <div class="contact-add-info">
                                    <div class="single-contact-add-info">
                                        <h3>Destek</h3>
                                        <p>Bize yazabilirsiniz. Çok seviniriz.</p>
                                    </div>
                                    <div class="single-contact-add-info">
                                        <h3>email</h3>
                                        <p>info@cvupp.com</p>
                                    </div>
                                    @*<div class="single-contact-add-info">
                                            <h3>website</h3>
                                            <p>www.brownsine.com</p>
                                        </div>*@
                                </div>
                            </div><!--/.contact-adress-->
                            <div class="hm-foot-icon">
                                <ul>
                                    @{
                                        string tiktok = "https://www.tiktok.com/@cvupp?";
                                    }
                                    <li><a href="https://www.linkedin.com/company/cvupp"><i class="fab fa-linkedin"></i></a></li><!--/li-->
                                    <li><a href="https://www.instagram.com/cvupp.tr/"><i class="fab fa-instagram"></i></a></li><!--/li-->
                                    <li><a href="@tiktok"><i class="fab fa-tiktok"></i></a></li><!--/li-->

                                </ul><!--/ul-->
                            </div><!--/.hm-foot-icon-->
                        </div><!--/.single-contact-box-->
                    </div><!--/.col-->
                </div><!--/.row-->
            </div><!--/.contact-content-->
        </div><!--/.container-->

    </section><!--/.contact-->
